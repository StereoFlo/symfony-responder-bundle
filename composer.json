{
    "name": "stereoflo/responder",
    "type": "symfony-bundle",
    "description": "Responder",
    "minimum-stability": "stable",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "stereoflo\\responder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stereoflo\\responder\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel":  "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/config":  "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.4"
    }
}
